@model IEnumerable<WydarzeniaKulturalne.Data.Entities.Bilety>
<!-- Dodałem przykładowy kod HTML do wyświetlenia wyników wyszukiwania -->
<h2>Wyniki wyszukiwania</h2>

@if (Model != null && Model.Count() > 0)
{
    <table class="table">
        <thead>
            <tr>
                <th>ID Biletu</th>
                <th>Nazwa Wydarzenia</th>
                <th>Data Wydarzenia</th>
                <!-- Dodaj inne kolumny według potrzeb -->
            </tr>
        </thead>
        <tbody>
            @foreach (var bilet in Model)
            {
                <tr>
                    <td>@bilet.Id</td>
                    <td>@bilet.Wydarzenie?.Nazwa</td>
                    <td>@bilet.DataWydarzenia.ToString("dd.MM.yyyy HH:mm")</td>
                    <!-- Dodaj inne kolumny według potrzeb -->
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>Brak wyników wyszukiwania.</p>
}
